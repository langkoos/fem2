<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config SYSTEM "http://www.matsim.org/files/dtd/config_v2.dtd">
<config>
	<module name="FEM" >
		<!-- FEM run type.  Options: justRunInitialPlansFile optimizeSafeNodesByPerson optimizeSafeNodesBySubsector  -->
		<param name="FEMRunType" value="justRunInitialPlansFile" />
		<param name="inputSubsectorsShapefile" value="subsectorshapefile/hn_evacuationmodel_PL2016_V12subsectorsVehic2016.shp" />
		<!-- Shapefile that contains the subsector shapes, and the number of vehicles per subsector.  Also contains connection to a network node, although MATSim would not really need that. -->
		<param name="inputSubsectorsToSafeNodesMappingsFile" value="2016_subsectors_safe_node_mapping.txt" />
		<!-- Hydrograph shapefile. This file is assumed to be projection EPSG:28356 and requires the following fields:
			ID       Integer unique id of point
			links_ids     [sic, this typo now sticks] String referring to EMME link ids.
			SUBSECTOR        String of the subsector name, needs to match exactly in other files.
			ALT_AHD          Floating point value, when hydrograph at this coordinate is greater than this value, link/subsector is assumed to be flooded. -->
		<param name="hydrographShapeFile" value="null" />
		<param name="sampleSize" value="0.01" />

	</module>

	<module name="network" >

		<param name="inputNetworkFile" value="hn_net_ses_emme_2016_V12_network.xml.gz" />

		<param name="inputChangeEventsFile" value="d09693_H_change_events.xml.gz" />
		<param name="timeVariantNetwork" value="true" />

	</module>

	<module name="plans" >
		<param name="inputPlansFile" value="plans_from_hn_evacuationmodel_PL2016_V12subsectorsVehic2016.xml.gz" />
	</module>

</config>
